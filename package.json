{
  "name": "draft-js-live-code-plugin",
  "version": "0.0.0",
  "description": "Live Code Features for DraftJS",
  "author": "asmr.hex",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "github.com/asmr-hex/draft-js-live-code-plugin"
  },
  "keywords": [
    "editor",
    "wysiwyg",
    "draft",
    "react",
    "react-component",
    "ide",
    "autocomplete",
    "highlighting",
    "interpreter",
    "live code"
  ],
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "scripts": {
    "start": "concurrently \"npm run watch:lib\" \"npm run watch:examples\"",
    "watch:examples": "npx webpack serve --open",
    "watch:lib": "npx tsc --watch",
    "build:lib": "npx tsc",
    "build:examples": "npx webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "draft-js": "^0.11.0",
    "react": "^16.0.0 || ^17.0.0",
    "react-dom": "^16.0.0 || ^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@types/jest": "^26.0.21",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "react-router-dom": "^5.2.0",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "typedoc": "^0.20.33",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@draft-js-plugins/editor": "^4.1.0",
    "@types/draft-js": "^0.11.0"
  }
}
